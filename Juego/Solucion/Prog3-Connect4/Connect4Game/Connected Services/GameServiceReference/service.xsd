    </xs:complexType>
  </xs:element>
  <xs:element name="UpdateGameStatus">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="status" type="xs:boolean" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="GetOpponentName">
    <xs:complexType>
      <xs:sequence>
        <xs:element minOccurs="0" name="oppName" nillable="true" type="xs:string" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>